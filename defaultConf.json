{
  "colourScheme": "everforest",

  "bar": {
    "left": [
      "menu",
      "workspaces",
      "activeWindow"
    ],
    "centre": [
      "dateAndTime"
    ],
    "right": [
      "network",
      "battery",
      "media",
      "volume"
    ],
    "autohide": false,
    "docked": false,
    "floatingModules": false,
    "multiColourModules": false,
    "moduleOutlines": true,
    "backgroundOutline": true,
    "workspaceCount": 10,
    "truncationLength": 60
  },

  "menu": {
    "width": 600,
    "capitaliseUsername": true,
    "capitaliseHostname": false,
    "dimBackground": true
  },

  "desktop": {
    "wallpaper": "",
    "videoWallpaper": false,
    "fadeSpeed": 2000,
    "shader": "",
    "hideWallpaper": false,
    "bgColour": "black",
    "clockWidget": false,
    "centreClockWidget": false,
    "autohideWidgets": false,
    "autohideBar": false,
    "autohideCursor": false
  },

  "notifications": {
    "width": 500,
    "defaultTimeout": 5000,
    "defaultCriticalTimeout": 8000,
    "sounds": true,
    "normalSound": "/usr/share/sounds/ocean/stereo/message-new-instant.oga",
    "criticalSound": "/usr/share/sounds/ocean/stereo/dialog-error-critical.oga",
    "dismissThreshold": 30
  },

  "osd": {
    "backlightName": ""
  }
}
